{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": false,
		"strict": true,
		"moduleResolution": "bundler",
		"types": [
			"node"
		]
	},
	"include": [
		"src/**/*",
		"src/**/*.svelte"
	],
	"exclude": [
		"node_modules/*",
		".svelte-kit/**/*"
	]
}